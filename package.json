  "scripts": {
    "dev": "concurrently \"npm run dev:build\" \"npm run dev:serve\" \"npm run floww\"",
    "components": "cd @thewickedwebdev/components && npm run dev",
    "components:patch": "cd @thewickedwebdev/components/ && npm version patch && npm publish",
    "components:minor": "cd @thewickedwebdev/components/  && npm version minor && npm publish",
    "components:major": "cd @thewickedwebdev/components/  && npm version major && npm publish",
    "dev:build": "NODE_ENV=development webpack --watch",
    "dev:serve": "nodemon dist --verbose",
    "build": "webpack --mode=production",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "flow": "flow",
    "floww": "flow-watch",
    "flow-typed": "flow-typed install",
    "test": "NODE_ENV=test BABEL_ENV=test jest ./src --detectOpenHandles --json --outputFile=.storybook/test-results.json",
    "test:watch": "NODE_ENV=test && BABEL_ENV=test && jest -o --watch --config=jest.config.js",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "buildstorybook": "NODE_ENV=development build-storybook -c ./.storybook -o storybook-static"
  },
